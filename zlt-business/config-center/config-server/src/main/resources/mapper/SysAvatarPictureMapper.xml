<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.central.config.mapper.SysAvatarPictureMapper">
    <select id="avatarPictureInfo" resultType="String">
        select a.url as url from sys_avatar_picture a
        LIMIT 1
    </select>

</mapper>